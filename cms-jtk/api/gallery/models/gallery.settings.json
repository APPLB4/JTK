{
  "kind": "collectionType",
  "collectionName": "gallery",
  "info": {
    "name": "gallery"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 1
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "video": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
